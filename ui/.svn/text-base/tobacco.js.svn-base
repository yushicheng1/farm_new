"use strict";
var aUi = aUi || {};
aUi.tobacco = {

	_laws: function(data) {
		var html = '<li data-id="' + data.id + '" class="mui-table-view-cell mui-media">' +
			'<span>' + data.name + '</span>'
		'	</li>';
		return html;
	},
	lawsList: function(dataList) {
		var html = "";
		for(var i = 0; i < dataList.length; i++) {
			html += this._laws(dataList[i]);
		}
		return html;
	},

